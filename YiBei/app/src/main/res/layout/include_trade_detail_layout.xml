<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/incloud_division_1dp_layout" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:gravity="center_vertical"
        android:paddingLeft="15px">

        <TextView
            android:id="@+id/tv_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/order_number"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_order_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/tv_one"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />


    </com.zhy.autolayout.AutoRelativeLayout>

    <include layout="@layout/incloud_division_1dp_layout" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:paddingLeft="15px">

        <TextView
            android:id="@+id/tv_pending_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/opening_price"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_open_price"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_pending_style"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

        <View
            android:id="@+id/view_one"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:background="@color/bg_or_division_color" />

        <TextView
            android:id="@+id/tv_three"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/view_one"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="@string/current_price"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_current_price"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_three"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <include layout="@layout/incloud_division_1dp_layout" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:paddingLeft="15px">

        <TextView
            android:id="@+id/tv_five"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/trade_times"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_trade_lots"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_five"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

        <View
            android:id="@+id/view_two"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:background="@color/bg_or_division_color" />

        <TextView
            android:id="@+id/tv_six"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/view_two"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="@string/margin_and_null"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_margin"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_six"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <include layout="@layout/incloud_division_1dp_layout" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:paddingLeft="15px">

        <TextView
            android:id="@+id/tv_seven"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="@string/stop_loss_price"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_stop_loss_pri"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_seven"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

        <View
            android:id="@+id/view_three"
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:background="@color/bg_or_division_color" />

        <TextView
            android:id="@+id/tv_eight"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/view_three"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:text="@string/stop_profit_price"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

        <TextView
            android:id="@+id/tv_stop_profit_pri"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/tv_eight"
            android:gravity="center_vertical"
            android:paddingLeft="55px"
            android:text="000000"
            android:textColor="@color/one_text_color"
            android:textSize="@dimen/text_size_26" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <include layout="@layout/incloud_division_1dp_layout" />

    <com.zhy.autolayout.AutoRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:gravity="center_vertical"
        android:paddingLeft="15px">

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="时间：0000-00-00  00:00:00"
            android:textColor="@color/two_text_color"
            android:textSize="@dimen/text_size_26" />

    </com.zhy.autolayout.AutoRelativeLayout>

    <include layout="@layout/incloud_division_1dp_layout" />


</com.zhy.autolayout.AutoLinearLayout>